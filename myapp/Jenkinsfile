pipeline
{
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                echo 'myapp/myapp.csproj'
                echo 'i love programing'
                script{
                   now=`date`
                   echo $now
                }
                
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
